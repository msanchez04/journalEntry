{
  "name": "gemini-schedule-demo",
  "version": "1.0.0",
  "description": "A demo application showing how to use Google's Gemini API to create student schedules",
  "main": "dist/concertStatsAI-tests.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/concertStatsAI-tests.js",
    "dev": "ts-node --transpile-only src/concertStatsAI-tests.ts",
    "test": "npm run build && node dist/concertStatsAI-tests.js",
    "manual": "npm run build && node -e \"require('./dist/concertStatsAI-tests.js').testManualEntry()\"",
    "llm": "npm run build && node -e \"require('./dist/concertStatsAI-tests.js').testAISummary()\"",
    "validators": "npm run build && node -e \"require('./dist/concertStatsAI-tests.js').testValidationFailures()\""
  },
  "keywords": [
    "gemini",
    "ai",
    "concert",
    "journal",
    "summary",
    "typescript"
  ],
  "author": "Student Demo",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
}
